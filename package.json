{
  "name": "gphotos-playwright",
  "version": "1.0.0",
  "description": "Playwright project for Google Photos automation",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "tsx index.ts",
    "restart": "while true; do tsx index.ts || [ $? -eq 1 ] && echo 'Restarting...' && sleep 2 || break; done",
    "fixremove": "tsx fixremove.ts",
    "scandates": "tsx fixdates.ts",
    "verifydates": "tsx verifydates.ts",
    "fillindates": "tsx fillInDates.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "playwright",
    "automation",
    "google-photos"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "playwright": "^1.40.0",
    "@playwright/test": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
